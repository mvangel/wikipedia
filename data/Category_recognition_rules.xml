<?xml version="1.0" encoding="UTF-8"?>
<ruleset>
	<!-- Rules of type="info" are applied only in text of Infoboxes.-->
	<rule>
		<pattern>(?m)^\s*\|\s*birth_</pattern>
		<category>Person</category>
	</rule>
	<rule>
		<pattern>(?m)^\s*\{\{\s*Geobox</pattern>
		<category>Location</category>
	</rule>
	<rule>
		<pattern>(?m)^\s*\{\{\s*Infobox\s*((company)|(.*organi[sz]ation))</pattern>
		<category>Organization</category>
	</rule>
	<rule type="info">
		<pattern>(?m)^\s*\|\s*[Ll]ocation</pattern>
		<category>Location</category>
	</rule>
	<!-- Weaker rules-->
	<rule>
		<pattern>(?m)^\s*\{\{\s*Persondata</pattern>
		<category>Person</category>
	</rule>
	<rule type="info">
		<pattern>(?m)^\s*\|\s*[Ll]atitude</pattern>
		<category>Location</category>
	</rule>
	<rule type="info">
		<pattern>(?m)^\s*\|\s*[Cc]oord[is]</pattern>
		<category>Location</category>
	</rule>
	<rule type="info">
		<pattern>(?m)^\s*\|\s*[Pp]opulation</pattern>
		<category>Location</category>
	</rule>
</ruleset>